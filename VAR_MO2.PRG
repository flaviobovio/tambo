LIN[ 4] = " 2   PARTO           FECHA      IDEN       T    NR   LAC   GES   ULT   ACUM  "
LIN[ 5] = "                     "+DTOC(FECPAR)+SPACE(3)+SEXO+SPACE(3)+TIPPAR+SPACE(4)+STR(NROPAR,2)+SPACE(3);
         +STR(DIALAC,3)+SPACE(3)+STR(DIAGES,3)+SPACE(3)+STR(ULTPRO,3)+SPACE(2)+STR(ACUPRO,5)+"  "
