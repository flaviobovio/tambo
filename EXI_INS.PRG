FUNCTION EXI_INS ( IN,VF )
PRIVATE CUR:=SETCURSOR(), BAS:=SELECT() , R:=.T.
SET CURSOR OFF
SELECT INSEMI
LOCATE FOR INSEMI = IN
IF ( FOUND() .AND. !VF ) .OR. ( !FOUND() .AND. VF )
   R=.F.
   TONE(136,4)
   SETCOLOR(INVER)
   @ 24, 1 SAY ' ESE INSEMINADOR ES INCORRECTO, REINGRESELO. CUALQUIER TECLA PARA CONTINUAR...  '
   SETCOLOR(COMUN)
   INKEY(0)
   @ 24, 0 CLEAR
ENDIF
SELECT( BAS )
SETCURSOR(CUR)
RETURN R