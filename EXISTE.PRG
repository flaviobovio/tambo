FUNCTION EXISTE ( CO )
PRIVATE CUR:=SETCURSOR() , R:=.T.
SET CURSOR OFF
SELECT TAMBO
SEEK CO
IF !FOUND()
   R=.F.
   TONE(136,4)
   SETCOLOR(INVER)
   @ 24, 2 SAY ' ESE CODIGO ES INCORRECTO, REINGRESELO. CUALQUIER TECLA PARA CONTINUAR... '
   SETCOLOR(COMUN)
   INKEY(0)
   @ 24, 0 CLEAR
ENDIF
SETCURSOR(CUR)
RETURN R