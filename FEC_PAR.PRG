FUNCTION FEC_PAR (FE)
PRIVATE CUR:=SETCURSOR() , R:=.T.  , BAS
SET CURSOR OFF
BAS=SELECT()
SELECT TAMBO
IF FE <= FECPAR
   R=.F.
   TONE(136,4)
   SETCOLOR(INVER)
   @ 24, 2 SAY ' ESA FECHA ES INCORRECTA, REINGRESELA . CUALQUIER TECLA PARA CONTINUAR... '
   SETCOLOR(COMUN)
   INKEY(0)
   @ 24, 0 CLEAR
ENDIF
SETCURSOR(CUR)
SELECT(BAS)
RETURN R