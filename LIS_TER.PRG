PROCEDURE LIS_TER()

PRIVATE LI, ENC, TE
CLS

@  0, 0 TO 23,79 DOUBLE
@  0, 2 SAY " LISTADOS DE GUACHERA "
@  7,31 TO 13,48 DOUBLE

@  9,33 PROMPT "1 - GENERAL   "
@ 11,33 PROMPT "2 - ENFERMERIA"
MENU TO LI

DO CASE
CASE LI = 1
   SELECT GUACHE
   ENC = 'LISTADO GENERAL DE GUACHERA'
CASE LI = 2
   SELECT ENFGUA
   ENC = 'LISTADO DE ENFERMERIA DE GUACHERA'
ENDCASE

COUNT TO TE
IF TE > 0
   VER_LIS( ENC )
ELSE
   MENSAJE("NO NINGUN TERNERO PARA LISTAR")
   INKEY(0)
ENDIF


