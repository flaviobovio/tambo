FUNCTION EXI_PRO ( CO )
PRIVATE CUR:=SETCURSOR() , R:=.T., BAS:=SELECT()
SET CURSOR OFF
SELECT TAMBO
SEEK CO
IF !FOUND() .OR. DIALAC<1
   R=.F.
   TONE(136,4)
   SETCOLOR(INVER)
   @ 24, 2 SAY ' ESE CODIGO ES INCORRECTO, REINGRESELO. CUALQUIER TECLA PARA CONTINUAR... '
   SETCOLOR(COMUN)
   INKEY(0)
   @ 24, 0 CLEAR
ENDIF
SELECT(BAS)
SETCURSOR(CUR)
RETURN R