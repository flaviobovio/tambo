FUNCTION ACH_FUN()
# INCLUDE 'ACHOICE.CH'
PARAMETER A_MOD
PRIVATE TECLA:=LASTKEY(), R
IF A_MOD = 3
   DO CASE
      CASE TECLA > 64 .AND. TECLA < 123
         R=AC_GOTO
      CASE TECLA = 13
         R=AC_SELECT
      OTHERWISE
         R=AC_CONT
   ENDCASE
ELSE
   R=AC_CONT
ENDIF
RETURN R